    "\n",
    "for i in range(10):\n",
    "    (c, t, o), (h, l) = wn.simulators.incarceration.run_sentence_length_trial(0, i)\n",
    "    print(np.sum(o), np.sum(h), np.sum(l), '-', nonzero(o), nonzero(h), nonzero(l))"
