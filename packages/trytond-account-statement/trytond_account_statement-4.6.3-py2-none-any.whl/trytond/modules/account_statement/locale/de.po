msgid "End Balance must be \"%s\"."
msgstr "Endsaldo muss \"%s\" sein."

msgctxt "error:account.statement:"
msgid "Number of Lines must be \"%s\"."
msgstr "Anzahl der Zeilen muss \"%s\" sein."

