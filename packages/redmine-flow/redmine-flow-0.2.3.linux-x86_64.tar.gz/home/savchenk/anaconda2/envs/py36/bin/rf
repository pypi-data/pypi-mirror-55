#!/home/savchenk/anaconda2/envs/py36/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'redmine-flow==0.2.3','console_scripts','rf'
__requires__ = 'redmine-flow==0.2.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('redmine-flow==0.2.3', 'console_scripts', 'rf')()
    )
