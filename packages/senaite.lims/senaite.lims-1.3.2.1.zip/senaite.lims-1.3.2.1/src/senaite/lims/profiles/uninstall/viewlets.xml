<?xml version="1.0"?>
<object>

  <order manager="plone.portaltop" skinname="*">
    <viewlet name="plone.header"/>
    <viewlet name="plone.personal_bar"/>
    <viewlet name="plone.app.i18n.locales.languageselector"/>
    <viewlet name="plone.path_bar"/>
  </order>

  <order manager="plone.app.layout.viewlets.interfaces.IAboveContentTitle" skinname="*">
    <viewlet name="bika.lims.import_button_viewlet"/>
  </order>

  <order manager="plone.portalfooter" skinname="*">
    <viewlet name="plone.footer"/>
    <viewlet name="bika.lims.authenticator"/>
    <viewlet name="plone.colophon"/>
    <viewlet name="plone.site_actions"/>
  </order>

  <order manager="plone.portalheader" skinname="*">
    <viewlet name="plone.skip_links"/>
    <viewlet name="plone.personal_bar"/>
    <viewlet name="plone.app.i18n.locales.languageselector"/>
    <viewlet name="plone.searchbox"/>
    <viewlet name="plone.logo"/>
    <viewlet name="plone.global_sections"/>
    <viewlet name="bika.lims.checknewversions"/>
  </order>

  <hidden manager="plone.abovecontent" skinname="*" purge="True">
    <viewlet name="plone.path_bar"/>
  </hidden>

  <hidden manager="plone.belowcontent" skinname="*" purge="True">
    <viewlet name="plone.belowcontenttitle.keywords"/>
  </hidden>

  <hidden manager="plone.portalfooter" skinname="*" purge="True">
  </hidden>

  <hidden manager="plone.portalheader" skinname="*" purge="True">
    <viewlet name="plone.personal_bar"/>
  </hidden>

</object>
