<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <title>{{ meta.get_value("title", "Wok") }}</title>

        <meta
            name="viewport"
            content="width=device-width, initial-scale=1, shrink-to-fit=no"
        />

        <link rel="stylesheet" href="/static/main.css" />
    </head>

    <body>
        <div class="wrapper">
            % if meta.get_value('emoji'):
            <div class="emoji">
                <span>{{ meta.get_value("emoji") }}</span>
            </div>
            % end

            {{ !html }}
        </div>
    </body>
</html>
