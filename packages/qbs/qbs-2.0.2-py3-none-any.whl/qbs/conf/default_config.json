{
	"cpp": {
		"build": ["g++", "--std=c++14", "-g", "{progname}{progext}", "-o", "{progname}"],
		"run": ["./{progname}"],
		"debug": ["gdb", "{progname}"],
		"cross_compile_windows": ["x86_64-w64-mingw32-g++", "--std=c++14", "-static", "-g", "{progname}{progext}", "-o", "{progname}.exe"],
		"t_auto": {
			"input": "-",
			"infiles": "{progname}.{num}.in",
			"output": "-",
			"outfiles": "{progname}.{num}.out",
			"begin": 0
		}
	},
	"java": {
		"build": ["javac", "{progname}{progext}"],
		"run": ["java", "{progname}"]
	},
	"python": {
		"build": [],
		"run": ["python", "{progname}{progext}"]
	},
	"python2": {
		"build": [],
		"run": ["python2", "{progname}{progext}"]
	},
	"python3": {
		"build": [],
		"run": ["python3", "{progname}{progext}"]
	},
	"py2": "python2",
	"py3": "python3",
	"py": "python"
}
