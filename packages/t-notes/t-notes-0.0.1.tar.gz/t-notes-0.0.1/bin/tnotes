#!/usr/bin/env python

import sys
import tnotes

try:
    if sys.argv[1] == "note":
        tnotes.edit_config({"note": sys.argv[2]})
    elif sys.argv[1] == "config":
        tnotes.edit_config({sys.argv[2]: sys.argv[3]})
    elif sys.argv[1] == "save":
        tnotes.save(sys.argv[2])
except IndexError:
    print("Not enough arguments")
