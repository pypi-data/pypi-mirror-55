
fact        = meaning / max_scoring / scoring / aftering

aftering    = after ws name ws name
meaning     = mean ws value ws value ws value
max_scoring = max_score ws name ws value
scoring     = score ws name ws value
after       = "after"
mean        = "mean"
score       = "score"
max_score   = "max-score"
name        = v_name / __var__
v_name      = ~"[a-z]+"
value       = v_value / __var__
v_value     = ~"\d*\.?\d+"
ws          = " "
