{
	"name": "kbw-keypad",
	"description": "This plugin attaches a popup keyboard to a text field for mouse-driven entry or adds an inline keypad in a division or span.",
	"keywords": [
		"i18n",
		"input",
		"keyboard",
		"keypad",
		"ui"
	],
	"homepage": "https://github.com/kbwood/keypad",
	"authors": [
		"Keith Wood <wood.keith@optusnet.com.au> (http://keith-wood.name)"
	],
	"licence": "MIT",
	"main": [
		"src/css/jquery.keypad.css",
		"src/js/jquery.keypad.js"
	],
	"ignore": [
		"**/.*",
		"node_modules",
		"src/bower_components"
	],
	"dependencies": {
		"jquery": ">=1.12.0",
		"kbw-plugin": "~1.0.3"
	},
	"devDependencies": {
		"qunit": "~1.23"
	}
}
