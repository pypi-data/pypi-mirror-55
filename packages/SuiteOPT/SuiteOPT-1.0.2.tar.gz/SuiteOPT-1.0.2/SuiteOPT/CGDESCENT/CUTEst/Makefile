#===============================================================================
# PASA/CUTEst/Makefile: For using PASA with CUTEst
#===============================================================================

default: cutest

#-------------------------------------
# Cutest
#-------------------------------------

cutest:
	./cg_install O3

#-------------------------------------
# Cutest with debug options
#-------------------------------------

debug:
	./cg_install debug

purge:
	- $(RM) cg_descent_main.o
