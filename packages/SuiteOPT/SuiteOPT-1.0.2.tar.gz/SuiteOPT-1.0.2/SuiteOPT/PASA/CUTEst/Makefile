#===============================================================================
# PASA/CUTEst/Makefile: For using PASA with CUTEst
#===============================================================================

default: cutest

#-------------------------------------
# Cutest
#-------------------------------------

cutest:
	./pasa_install O3

#-------------------------------------
# Cutest with debug options
#-------------------------------------

debug:
	./pasa_install debug

nopproj:
	./pasa_install nopproj

purge:
	- $(RM) pasa_main.o
