#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'v2sub==1.1','console_scripts','v2sub'
__requires__ = 'v2sub==1.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('v2sub==1.1', 'console_scripts', 'v2sub')()
    )
