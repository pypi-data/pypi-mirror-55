#!/home/nemo/miniconda3/envs/cli/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'CLI-passthrough==0.1.3','console_scripts','cli-passthrough'
__requires__ = 'CLI-passthrough==0.1.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('CLI-passthrough==0.1.3', 'console_scripts', 'cli-passthrough')()
    )
